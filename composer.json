{
  "name": "pierre-tranchard/cron-monitor-bundle",
  "license": "MIT",
  "type": "symfony-bundle",
  "description": "Bundle designed to monitor cron & command",
  "autoload": {
    "psr-4": {
      "Tranchard\\CronMonitorBundle\\": ""
    }
  },
  "keywords": ["cron", "api", "monitoring", "mongodb"],
  "autoload-dev": {
    "psr-4": {
      "Tranchard\\CronMonitorBundle\\Tests\\": ""
    }
  },
  "require": {
    "php": ">=7.1.3",
    "guzzlehttp/guzzle": "~6.0",
    "symfony/symfony": "^3.0 || ^4.0",
    "symfony/swiftmailer-bundle": "^3.0",
    "symfony/monolog-bundle": "^3.1"
  },
  "require-dev": {
    "symfony/phpunit-bridge": "^3.0 || ^4.0",
    "phpunit/phpunit": "^6.0"
  },
  "suggest": {
    "symfony/swiftmailer-bundle": "Allow to send fallback message via Swift Mailer"
  }
}
